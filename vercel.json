{
    "version": 2,
    "builds": [
        {
            "src": "backend/api/index.py",
            "use": "@vercel/python"
        },
        {
            "src": "index.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/backend/api/index.py"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}